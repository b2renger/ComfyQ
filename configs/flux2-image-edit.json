{
    "id": "flux2-image-edit",
    "name": "Flux2 Klein Image Edit (9B)",
    "description": "Edit images using reference images and prompts with Flux2 Klein 9B model",
    "server": {
        "port": 3000,
        "host": "0.0.0.0"
    },
    "comfy_ui": {
        "installation_type": "portable",
        "root_path": "D:/ComfyUI_windows_portable_nvidia/ComfyUI_windows_portable/ComfyUI",
        "python_executable": "../python_embeded/python.exe",
        "output_dir": "output",
        "api_host": "127.0.0.1",
        "api_port": 8188
    },
    "workflow": {
        "template_file": "./workflows/image_flux2_klein_image_edit_9b_base.json",
        "warmup_prompt": "A simple edit",
        "job_duration_ms": 45000,
        "parameter_map": {
            "image1": {
                "node_id": "TBD",
                "field": "image",
                "type": "image",
                "label": "Base Image",
                "default": null
            },
            "image2": {
                "node_id": "TBD",
                "field": "image",
                "type": "image",
                "label": "Reference Image",
                "default": null
            },
            "prompt": {
                "node_id": "TBD",
                "field": "text",
                "type": "textarea",
                "label": "Edit Prompt",
                "default": "Replace the background with elements from the reference image"
            },
            "steps": {
                "node_id": "TBD",
                "field": "steps",
                "type": "number",
                "label": "Steps",
                "default": 20,
                "min": 1,
                "max": 50
            },
            "width": {
                "node_id": "TBD",
                "field": "width",
                "type": "select",
                "label": "Width",
                "options": [
                    1024,
                    832,
                    1216,
                    768
                ],
                "default": 1024
            },
            "height": {
                "node_id": "TBD",
                "field": "height",
                "type": "select",
                "label": "Height",
                "options": [
                    1024,
                    1216,
                    832,
                    1344
                ],
                "default": 1024
            }
        }
    }
}